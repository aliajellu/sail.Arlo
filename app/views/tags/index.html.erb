<h1>Listing tags</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Uid</th>
    <th>Type</th>
    <th>Version</th>
    <th>Author</th>
    <th>Run</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tags.each do |tag| %>
  <tr>
    <td><%= tag.id %></td>
    <td><%= tag.uid %></td>
    <td><%= tag.type %></td>
    <td><%= tag.version %></td>
    <td><%= tag.author %></td>
    <td><%= tag.run_id %></td>
    <td><%= tag.name %></td>
    <td><%= link_to 'Show', tag %></td>
    <td><%= link_to 'Edit', edit_tag_path(tag) %></td>
    <td><%= link_to 'Destroy', tag, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tag', new_tag_path %>
